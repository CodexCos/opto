{
    "headers": [
      {
        "source": "/:path*", 
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=3600, immutable"  
          }
        ]
      },
      {
        "source": "/images/:path*", 
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"  
          }
        ]
      }
    ]
  }
  